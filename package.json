{
  "name": "get-ens",
  "version": "1.0.2",
  "description": "Get text records of an ENS address with ease.",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node --loader=esbuild-node-loader node_modules/uvu/bin.js tests",
    "build": "rollup -c",
    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talentlessguy/get-ens.git"
  },
  "keywords": [
    "ethereum",
    "ens",
    "blockchain",
    "eth"
  ],
  "author": "v1rtl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talentlessguy/get-ens/issues"
  },
  "homepage": "https://github.com/talentlessguy/get-ens#readme",
  "dependencies": {
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.5",
    "keccak": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/keccak": "^3.0.1",
    "@types/node": "^16.9.2",
    "c8": "^7.9.0",
    "esbuild-node-loader": "^0.3.1",
    "husky": "^7.0.2",
    "node-fetch": "^3.0.0",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "uvu": "^0.5.1"
  },
  "engines": {
    "node": ">=12.4"
  }
}
